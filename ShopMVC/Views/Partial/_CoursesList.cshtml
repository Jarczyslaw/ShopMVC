@using ShopMVC.DataAccess.Models;

@model IEnumerable<Course>

@{
    if (Model.Count() > 0)
    {
        <ul>
            @foreach (var c in Model)
            {
                <li><a href="">@c.Title</a></li>
            }
        </ul>
    }
    else
    {
        <p>No courses available here!</p>
    }
}


